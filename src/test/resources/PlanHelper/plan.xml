<planModel>
  <name>GSEFS_DDS</name>
  <dependency>
    <groupId>com.fedex.tibco.sf</groupId>
    <artifactId>sf-tibco-as-stack</artifactId>
    <version>2.1.4.2.0</version>
    <type>jar</type>
  </dependency>
  <archive name="PlanModel.zip">
    <path>/target/</path>
    <relativePath>fabric</relativePath>
  </archive>
  <model>
    <id>L1</id>
    <!-- Typical AS Configuration -->
    <!-- One Component with multiple stacks -->
    <properties>
      <sf.broker.url></sf.broker.url>
    </properties>

    <stack>
      <component>
        <description>GSEF DDS Metaspace for L1</description>
        <defaultSetting name="Default Max Engines" value="1" />
        <defaultSetting name="Default Min Engines" value="1" />
        <defaultSetting name="Default Priority" value="3" />
        <runtimeContextVariable name="AMI_HAWK_NETWORK" value="tcp:7474">
          <export>true</export>
          <type>1</type>
          <description>Describe</description>
          <autoIncrement>0</autoIncrement>
        </runtimeContextVariable>
        <properties>
          <assigned.hostname>urh00609</assigned.hostname>
        </properties>
      </component>
    </stack>
    <stack>
      <name>GSEFS_DDS_AS_L1_Stack#2</name>
      <component>
        <name>GSEFS_DDS_AS_L1</name>
        <properties>
          <as.stack.hostname>urh00609</as.stack.hostname>
        </properties>
      </component>
    </stack>
  </model>
  <model>
    <id>L2</id>
    <!-- Typical BW Configuration -->
    <!-- One Component with One Stack dependent on Admin -->
    <stack>
      <name>GSEFS_StopGateway_BW_L1_Stack</name>
      <componentDependency>
        <name>GSEFS_ADMIN_L1</name>
      </componentDependency>
      <component>
        <name>GSEFS_StopGateway_BW_L1</name>
        <properties>
          <as.stack.hostname>urh00600</as.stack.hostname>
        </properties>
      </component>
      <policy>
        <componentAllocationInfo>
          <name></name>
          <allocationRule>
          </allocationRule>
        </componentAllocationInfo>
        <scheduleName>9To5</scheduleName>
        <scheduled>true</scheduled>
      </policy>
    </stack>
  </model>
  <model>
    <id>L2</id>
    <!-- Typical BE Configuration -->
    <!-- Multiple Components with One Stack -->
    <stack>
      <name>GSEFS_StopCore_BE_L1_Stack</name>
      <component>
        <name>GSEFS_StopCore_BE_L1</name>
        <properties>
          <as.stack.hostname>urh00600</as.stack.hostname>
        </properties>
      </component>
    </stack>
    <stack>
      <name>GSEFS_StopCoreCache_BE_L1_Stack</name>
      <component>
        <name>GSEFS_StopCoreCache_BE_L1</name>
        <properties>
          <as.stack.hostname>urh00600</as.stack.hostname>
        </properties>
      </component>
    </stack>
  </model>
</planModel>